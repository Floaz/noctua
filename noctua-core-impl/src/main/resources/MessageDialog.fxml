<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?scenebuilder-stylesheet TestOptionsView.css?>

<VBox id="" styleClass="window" xmlns:fx="http://javafx.com/fxml">
  <Region styleClass="headerBox" VBox.vgrow="NEVER">
    <prefHeight>
      <Long fx:value="20" />
    </prefHeight>
    <prefWidth>
      <Long fx:value="-1" />
    </prefWidth>
  </Region>
  <HBox id="HBox" alignment="CENTER_LEFT" VBox.vgrow="ALWAYS">
    <children>
      <ImageView fx:id="icon" pickOnBounds="true" preserveRatio="true">
        <effect>
          <Reflection />
        </effect>
        <fitHeight>
          <Long fx:value="32" />
        </fitHeight>
        <fitWidth>
          <Long fx:value="32" />
        </fitWidth>
        <image>
          <Image url="@images/Get%20Info%20Blue%20Button.png" />
        </image>
      </ImageView>
      <VBox id="VBox" alignment="CENTER_LEFT" spacing="5.0" HBox.hgrow="ALWAYS">
        <children>
          <Label fx:id="descriptionLabel" style="&#10;" text="Text" textAlignment="JUSTIFY" wrapText="true">
            <prefHeight>
              <Long fx:value="-1" />
            </prefHeight>
            <prefWidth>
              <Long fx:value="-1" />
            </prefWidth>
          </Label>
        </children>
      </VBox>
    </children>
    <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <spacing>
      <Long fx:value="10" />
    </spacing>
  </HBox>
  <HBox alignment="CENTER" spacing="10.0" styleClass="bottomButtonBar">
    <Button id="startButton" fx:id="okButton" cancelButton="true" defaultButton="false" onAction="#handleOkButtonAction" style="-fx-padding: 6px;&#10;" text="Ok">
      <effect>
        <Reflection />
      </effect>
      <font>
        <Font name="System Bold" size="12.0" fx:id="x3" />
      </font>
      <graphic>
        <ImageView mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
          <fitHeight>
            <Long fx:value="16" />
          </fitHeight>
          <fitWidth>
            <Long fx:value="16" />
          </fitWidth>
          <image>
            <Image url="@images/Correct.png" preserveRatio="false" smooth="false" />
          </image>
        </ImageView>
      </graphic>
      <graphicTextGap>
        <Long fx:value="4" />
      </graphicTextGap>
    </Button>
  </HBox>
  <maxHeight>
    <Long fx:value="220" />
  </maxHeight>
  <maxWidth>
    <Long fx:value="380" />
  </maxWidth>
  <minHeight>
    <Long fx:value="220" />
  </minHeight>
  <minWidth>
    <Long fx:value="380" />
  </minWidth>
  <prefHeight>
    <Long fx:value="220" />
  </prefHeight>
  <prefWidth>
    <Long fx:value="380" />
  </prefWidth>
  <spacing>
    <Long fx:value="0" />
  </spacing>
  <stylesheets>
    <URL value="@General.css" />
  </stylesheets>
</VBox>
