<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?scenebuilder-stylesheet TestOptionsView.css?>

<VBox id="" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="350.0" minWidth="300.0" prefHeight="400.0" prefWidth="500.0" styleClass="window" xmlns:fx="http://javafx.com/fxml">
  <Region styleClass="headerBox" VBox.vgrow="NEVER">
    <prefHeight>
      <Long fx:value="20" />
    </prefHeight>
    <prefWidth>
      <Long fx:value="-1" />
    </prefWidth>
  </Region>
  <HBox id="HBox" alignment="CENTER_LEFT" VBox.vgrow="ALWAYS">
    <children>
      <ImageView fx:id="icon" pickOnBounds="true" preserveRatio="true">
        <effect>
          <Reflection />
        </effect>
        <fitHeight>
          <Long fx:value="32" />
        </fitHeight>
        <fitWidth>
          <Long fx:value="32" />
        </fitWidth>
        <image>
          <Image url="@images/Mr.%20Bomb.png" />
        </image>
      </ImageView>
      <VBox id="VBox" alignment="CENTER_LEFT" spacing="5.0" HBox.hgrow="ALWAYS">
        <children>
          <Label fx:id="descriptionLabel" style="&#10;" text="Es ist leider ein Programmfehler aufgetreten!" textAlignment="JUSTIFY" textFill="#990000" wrapText="true">
            <font>
              <Font name="System Bold" size="15.0" />
            </font>
            <prefHeight>
              <Long fx:value="-1" />
            </prefHeight>
            <prefWidth>
              <Long fx:value="-1" />
            </prefWidth>
          </Label>
          <Label text="Fehlermeldung:" />
          <TextField id="textField" fx:id="messageField" editable="false" minHeight="30.0" text="Unbekannte Fehlermeldung">
            <prefWidth>
              <Long fx:value="-1" />
            </prefWidth>
          </TextField>
          <Label text="Details:" />
          <TextArea fx:id="fullExceptionField" editable="false" prefWidth="200.0" wrapText="true" VBox.vgrow="ALWAYS" />
          <Label prefHeight="120.0" text="Du kannst die Fehlermeldung ignorieren und das Programm weiterbenutzen. Vielleicht ist dir aber lieber, wenn das Programm nicht weiterlÃ¤uft, in diesem Fall, kannst du Noctua auch direkt beenden." wrapText="true" />
        </children>
      </VBox>
    </children>
    <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <spacing>
      <Long fx:value="10" />
    </spacing>
  </HBox>
  <HBox alignment="CENTER_LEFT" spacing="10.0" styleClass="bottomButtonBar">
    <Button id="startButton" fx:id="cancelButton" onAction="#handleCancelButtonAction" style="-fx-padding: 6px;&#10;" text="Programm beenden">
      <effect>
        <Reflection />
      </effect>
      <font>
        <Font size="12.0" />
      </font>
      <graphic>
        <ImageView fitHeight="24.0" fitWidth="24.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
          <image>
            <Image url="@images/cancel.png" preserveRatio="false" smooth="false" />
          </image>
        </ImageView>
      </graphic>
      <graphicTextGap>
        <Long fx:value="4" />
      </graphicTextGap>
    </Button>
    <HBox id="HBox" alignment="CENTER_RIGHT" spacing="5.0" HBox.hgrow="ALWAYS">
      <children>
        <Button id="startButton" fx:id="okButton" onAction="#handleOkButtonAction" style="-fx-padding: 6px;&#10;" text="Ignorieren">
          <effect>
            <Reflection />
          </effect>
          <font>
            <Font name="System Bold" size="12.0" fx:id="x3" />
          </font>
          <graphic>
            <ImageView fitHeight="24.0" fitWidth="24.0" mouseTransparent="true" pickOnBounds="true" preserveRatio="true">
              <image>
                <Image url="@images/Play.png" />
              </image>
            </ImageView>
          </graphic>
          <graphicTextGap>
            <Long fx:value="4" />
          </graphicTextGap>
        </Button>
      </children>
    </HBox>
  </HBox>
  <spacing>
    <Long fx:value="0" />
  </spacing>
  <stylesheets>
    <URL value="@General.css" />
  </stylesheets>
</VBox>
